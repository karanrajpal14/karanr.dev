    "gatsby-remark-images": "^4.2.0",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-transformer-json": "^3.2.0",
    "gatsby-transformer-sharp": "^3.2.0",
    "node-sass": "^5.0.0",
    "path": "^0.12.7",
    "prism-react-renderer": "^1.0.2",
    "rbx": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-live": "^2.2.2",
