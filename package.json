{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "gen-header": "pageres http://localhost:9000 1366x768 --selector='.hero' --delay=1.5 --filename='static/header' --format='jpg' --overwrite",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean && rimraf node_modules/ yarn.lock && yarn",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.3",
    "@mdx-js/react": "^1.5.3",
    "axios": "^0.19.1",
    "babel-plugin-styled-components": "^1.10.6",
    "bulma-timeline": "^3.0.4",
    "dotenv": "^8.2.0",
    "formik": "^2.1.1",
    "gatsby": "^2.20.2",
    "gatsby-image": "^2.3.0",
    "gatsby-plugin-anchor-links": "^1.1.1",
    "gatsby-plugin-manifest": "^2.3.1",
    "gatsby-plugin-mdx": "^1.1.0",
    "gatsby-plugin-offline": "^3.1.0",
    "gatsby-plugin-react-helmet": "^3.2.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sass": "^2.2.0",
    "gatsby-plugin-scroll-indicator": "^1.0.0",
    "gatsby-plugin-scroll-reveal": "^0.0.6",
    "gatsby-plugin-sharp": "^2.5.1",
    "gatsby-plugin-sitemap": "^2.3.0",
    "gatsby-plugin-styled-components": "^3.1.16",
    "gatsby-remark-images": "^3.2.0",
    "gatsby-source-filesystem": "^2.2.1",
    "gatsby-transformer-json": "^2.3.0",
    "gatsby-transformer-sharp": "^2.4.1",
    "node-sass": "^4.13.0",
    "prism-react-renderer": "^1.0.2",
    "rbx": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-live": "^2.2.2",
    "react-seo-component": "^1.0.0",
    "rimraf": "^3.0.2",
    "styled-components": "5.0.1",
