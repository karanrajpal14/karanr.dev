{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "gen-header": "pageres http://localhost:9000 1366x768 --selector='.hero' --delay=1.5 --filename='static/header' --format='jpg' --overwrite",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean && rimraf node_modules/ package-lock.json && npm i",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@fontsource/aleo": "^4.2.2",
    "@fontsource/cooper-hewitt": "^4.2.4",
    "@mdx-js/mdx": "^1.5.3",
    "@mdx-js/react": "^1.5.3",
    "axios": "^0.21.1",
    "babel-plugin-styled-components": "^1.10.6",
    "bulma-timeline": "^3.0.4",
    "dotenv": "^8.2.0",
    "formik": "^2.1.1",
    "gatsby": "^3.2.1",
    "gatsby-image": "^3.2.0",
    "gatsby-plugin-anchor-links": "^1.1.1",
    "gatsby-plugin-manifest": "^3.2.0",
    "gatsby-plugin-mdx": "^2.2.0",
    "gatsby-plugin-offline": "^4.2.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-root-import": "^2.0.5",
